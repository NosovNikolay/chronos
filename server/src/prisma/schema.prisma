generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model User {
  id    String @db.Uuid @id @default(uuid())
  email String @unique @db.VarChar(50)
  username String @db.VarChar(50)
  password String @db.VarChar(255)
}
